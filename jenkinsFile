	pipeline{
	   agent any
		  stages {
			 stage('compile') {
				steps{
				   withMaven(maven : 'Maven 3.6'){
					  bat 'mvn compile'
				   }
				}
			 }
			 stage('package') {
				steps{
				   withMaven(maven : 'Maven 3.6'){
					  bat 'mvn package -DskipTests'
				   }
				}
			 }
			 stage('install') {
				steps{
				   withMaven(maven : 'Maven 3.6'){
					  bat 'mvn install -DskipTests'
				   }
				}
			 }
		  }
	}
